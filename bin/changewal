#! /bin/sh
imagepath="$HOME/Pictures/wall/"
wall=$(ls $imagepath | dmenu -i  -p "Select wall")
[ ! -z "$imagepath$wall" ] && cp "$imagepath$wall" ~/.config/wall.png && notify-send -i "$HOME/.config/wall.png" "Wallpaper changed."
   xwallpaper --zoom ~/.config/wall.png
   betterlockscreen -u ~/.config/wall.png

