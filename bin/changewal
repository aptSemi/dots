#! /bin/sh
imagepath="$HOME/Pictures/wallpapers/"
category=$(ls $imagepath | dmenu -i  -p "Select category")
wall=$(ls $imagepath/$category | dmenu -i  -p "Select wall")
[ ! -z "$imagepath$category/$wall" ] && cp "$imagepath$category/$wall" ~/.config/wall.png && notify-send -i "$HOME/.config/wall.png" "Wallpaper changed." 
feh --bg-fill $HOME/.config/wall.png &&
betterlockscreen -u $imagepath$category/$wall

